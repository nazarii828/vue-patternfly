{
  "name": "@vue-patternfly/core",
  "version": "0.1.0-beta.4",
  "files": [
    "dist",
    "global.d.ts"
  ],
  "main": "dist/core.umd.js",
  "module": "dist/core.es.js",
  "typings": "dist/types/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/core.es.js",
      "require": "./dist/core.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "watch": "concurrently 'npm:watch-*'",
    "watch-vite": "vite build --watch",
    "watch-tsc": "vue-tsc --declaration --emitDeclarationOnly --watch",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.0.2",
    "@patternfly/patternfly": "^4.217.0",
    "@patternfly/react-styles": "^4.91.5",
    "@patternfly/react-tokens": "^4.93.5",
    "@vue-patternfly/icons": "^0.2.0",
    "@vueuse/core": "^9.3.0",
    "@vueuse/integrations": "^9.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/compiler-sfc": "^3.2.40",
    "concurrently": "^7.4.0",
    "focus-trap": "^7.0.0",
    "fs-extra": "^10.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5",
    "vue-tsc": "^0.40.13"
  },
  "peerDependencies": {
    "focus-trap": "*",
    "vue": "^3"
  }
}
