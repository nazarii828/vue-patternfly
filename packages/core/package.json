{
  "name": "@vue-patternfly/core",
  "version": "0.1.0-beta.15",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "types": "./dist/main.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "concurrently 'npm:build:*'",
    "build:vite": "vite build",
    "build:bundle": "vite build -c vite-bundle.config.ts",
    "build:declarations": "vue-tsc --declaration --emitDeclarationOnly && node scripts/build-global.mjs",
    "build:cjs": "tsc --build --verbose tsconfig.cjs.json",
    "build:esm": "tsc --build --verbose tsconfig.json",
    "lint": "vue-tsc --noEmit",
    "watch": "concurrently 'npm:watch-*'",
    "watch-vite": "vite build --watch",
    "watch-tsc": "vue-tsc --declaration --emitDeclarationOnly --watch",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@floating-ui/core": "^1.5.0",
    "@floating-ui/dom": "^1.5.3",
    "@patternfly/patternfly": "^5.1.0",
    "@patternfly/react-styles": "^5.1.1",
    "@patternfly/react-tokens": "^5.1.1",
    "@vue-patternfly/icons": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "@types/web-bluetooth": "^0.0.19",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/compiler-sfc": "^3.3.7",
    "concurrently": "^8.2.2",
    "focus-trap": "^7.5.4",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.22",
    "@vueuse/core": "^10.5.0",
    "@vueuse/integrations": "^10.5.0"
  },
  "peerDependencies": {
    "@vueuse/integrations": "^10",
    "focus-trap": "*"
  },
  "packageManager": "pnpm@8.6.1"
}
