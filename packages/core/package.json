{
  "name": "@vue-patternfly/core",
  "version": "0.1.0-beta.4",
  "files": [
    "dist",
    "global.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/esm/main.d.ts",
      "import": "./dist/core.es.js",
      "require": "./dist/core.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "concurrently 'npm:build:*'",
    "build:vite": "vite build",
    "build:declarations": "vue-tsc --declaration --emitDeclarationOnly",
    "build:cjs": "tsc --build --verbose tsconfig.cjs.json",
    "build:esm": "tsc --build --verbose tsconfig.json",
    "lint": "vue-tsc --noEmit",
    "watch": "concurrently 'npm:watch-*'",
    "watch-vite": "vite build --watch",
    "watch-tsc": "vue-tsc --declaration --emitDeclarationOnly --watch",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@floating-ui/core": "^1.0.2",
    "@floating-ui/dom": "^1.0.6",
    "@patternfly/patternfly": "^4.219.2",
    "@patternfly/react-styles": "^4.91.10",
    "@patternfly/react-tokens": "^4.93.10",
    "@vue-patternfly/icons": "workspace:^0.2.0",
    "@vueuse/core": "^9.5.0",
    "@vueuse/integrations": "^9.5.0",
    "@vueuse/shared": "^9.5.0"
  },
  "devDependencies": {
    "@types/web-bluetooth": "^0.0.16",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.2.45",
    "concurrently": "^7.5.0",
    "focus-trap": "^7.1.0",
    "fs-extra": "^10.1.0",
    "rimraf": "^3.0.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9"
  },
  "peerDependencies": {
    "focus-trap": "*",
    "vue": "^3"
  },
  "packageManager": "pnpm@7.13.0"
}
