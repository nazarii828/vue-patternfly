{
  "name": "@vue-patternfly/core",
  "version": "0.1.0-beta.12",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "types": "./dist/main.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "concurrently 'npm:build:*'",
    "build:vite": "vite build",
    "build:bundle": "vite build -c vite-bundle.config.ts",
    "build:declarations": "vue-tsc --declaration --emitDeclarationOnly && node scripts/build-global.mjs",
    "build:cjs": "tsc --build --verbose tsconfig.cjs.json",
    "build:esm": "tsc --build --verbose tsconfig.json",
    "lint": "vue-tsc --noEmit",
    "watch": "concurrently 'npm:watch-*'",
    "watch-vite": "vite build --watch",
    "watch-tsc": "vue-tsc --declaration --emitDeclarationOnly --watch",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@floating-ui/core": "^1.3.1",
    "@floating-ui/dom": "^1.4.5",
    "@patternfly/patternfly": "^4.224.2",
    "@patternfly/react-styles": "^4.92.6",
    "@patternfly/react-tokens": "^4.94.6",
    "@vue-patternfly/icons": "workspace:^",
    "@vueuse/core": "^10.2.1",
    "@vueuse/integrations": "^10.2.1",
    "@vueuse/shared": "^10.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/web-bluetooth": "^0.0.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "concurrently": "^8.2.0",
    "focus-trap": "^7.5.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.3",
    "rimraf": "^5.0.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.5"
  },
  "peerDependencies": {
    "focus-trap": "*",
    "vue": "^3"
  },
  "packageManager": "pnpm@8.6.1"
}
