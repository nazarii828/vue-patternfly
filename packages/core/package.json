{
  "name": "@vue-patternfly/core",
  "version": "0.1.0-beta.17",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js",
      "types": "./dist/main.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "concurrently 'npm:build:*'",
    "build:vite": "vite build",
    "build:bundle": "vite build -c vite-bundle.config.ts",
    "build:declarations": "vue-tsc --declaration --emitDeclarationOnly && node scripts/build-global.mjs",
    "build:cjs": "tsc --build --verbose tsconfig.cjs.json",
    "build:esm": "tsc --build --verbose tsconfig.json",
    "lint": "vue-tsc --noEmit",
    "watch": "concurrently 'npm:watch:*'",
    "watch:vite": "vite build --watch",
    "watch:tsc": "vue-tsc --declaration --emitDeclarationOnly --watch",
    "watch:main.d.ts": "nodemon --watch dist/main.d.ts --watch 'dist/**/index.d.ts' --watch scripts/build-global.mjs -x \"node scripts/build-global.mjs\"",
    "prepack": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@floating-ui/core": "^1.5.2",
    "@floating-ui/dom": "^1.5.3",
    "@patternfly/patternfly": "^5.1.0",
    "@patternfly/react-styles": "^5.1.2",
    "@patternfly/react-tokens": "^5.1.2",
    "@vue-patternfly/icons": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/web-bluetooth": "^0.0.20",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/compiler-sfc": "^3.3.11",
    "@vueuse/core": "^10.7.0",
    "@vueuse/integrations": "^10.7.0",
    "@vueuse/shared": "^10.7.0",
    "concurrently": "^8.2.2",
    "focus-trap": "^7.5.4",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.25"
  },
  "peerDependencies": {
    "@vueuse/core": "^10",
    "@vueuse/integrations": "^10",
    "@vueuse/shared": "^10",
    "focus-trap": "*"
  },
  "packageManager": "pnpm@8.6.1"
}
