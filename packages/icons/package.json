{
  "name": "@vue-patternfly/icons",
  "version": "0.3.0",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/js/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./*": {
      "import": "./dist/esm/icons/*.js",
      "require": "./dist/js/icons/*.js",
      "types": "./dist/esm/icons/*.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist src/icons src/index.js src/index.d.ts",
    "build": "concurrently 'npm:build:*'",
    "build:icons": "node scripts/build.js",
    "build:cjs": "tsc --build --verbose tsconfig.cjs.json",
    "build:esm": "tsc --build --verbose tsconfig.json",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-modules-commonjs": "^7.22.5",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@patternfly/patternfly": "^4.224.2",
    "concurrently": "^8.2.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.7",
    "rimraf": "^5.0.1",
    "ts-patch": "^2.1.0",
    "tslib": "^2.5.3",
    "typescript": "~5.0.4",
    "vue": "^3.3.4"
  },
  "peerDependencies": {
    "vue": "^3.2.40"
  },
  "packageManager": "pnpm@8.6.1"
}
