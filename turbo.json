{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "clean": {
      "dependsOn": [
        "^clean"
      ],
      "outputs": []
    },
    "build": {
      "dependsOn": [
        "clean",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": [],
      "inputs": [
        "src/**/*.vue",
        "src/**/*.ts"
      ]
    },
    "lint": {
      "outputs": []
    }
  }
}
